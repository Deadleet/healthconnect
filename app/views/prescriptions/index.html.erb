<div class = "container">
  <h1 class = "">Vos prescriptions</h1>
  <ul class = "card-product">
    <% @prescriptions.each do |prescription| %>
      <% if (Date.today <= prescription.end_date && prescription.start_date <= Date.today) %>
        <div class = "card-title h2">
        <%= link_to prescription.title, prescription_path(prescription) %>
        </div>
        <div class = "card-description" p>
        <ul>Description:<%= prescription.description %></ul>
        <ul>Expiration:<%= prescription.end_date %></ul>
        </div>
        <% else %>
        <%# <div>
        <p class = "card-title h2"> Vous n'avez pas de prescription </p>
        </div> %>
      <% end %>
    <% end %>
  </ul>
