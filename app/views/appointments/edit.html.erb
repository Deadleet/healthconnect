<div class="background">
  <div class="bg-white rounded p-3 shadow">
    <h1 class="edit-form">Modifier vos pathologies</h1>
      <div class="form">
      <%= simple_form_for @appointment do |f| %>
      <div>
      <%= f.input :titre, placeholder: 'Titre du rendez-vous'%>
      <%= f.input :date, placeholder: 'date' %>
      </div>
        <%= f.submit 'edit',class: 'btn btn-ghost' %>
      <% end %>
      </div>
  </div>
</div>
